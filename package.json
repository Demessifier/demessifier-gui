{
  "name": "@demessifier/demessifier-gui",
  "version": "0.0.0-development",
  "description": "Demessifier GUI framework",
  "scripts": {
    "clean:node": "npx rimraf package-lock.json && npx rimraf pnpm-lock.json && npx rimraf node_modules",
    "clean:cache": "npx rimraf --glob auxiliary/*",
    "clean:test": "npx rimraf --glob test/vitest/* && npx rimraf --glob test/cypress/screenshots/* && npx rimraf --glob test/cypress/videos/* && npx rimraf --glob test/cypress/downloads/*",
    "clean:build": "npx rimraf --glob build/*",
    "clean:generate": "npx rimraf src/css/default-css-variables.css",
    "clean": "pnpm run clean:node && pnpm run clean:cache && pnpm run clean:test && pnpm run clean:build && pnpm run clean:generate",
    "packages:install": "pnpm install",
    "packages:update": "pnpm update",
    "packages:audit": "pnpm audit",
    "packages:audit-fix": "pnpm audit fix",
    "generate:default-css-variables": "npx tsx src/css/default-css-variables.ts",
    "generate": "pnpm run generate:default-css-variables",
    "dev": "vite --host 0.0.0.0",
    "lint:format": "prettier --log-level warn --cache --cache-strategy content --cache-location auxiliary/prettier-cache --write \"./**/*.{astro,cjs,css,js,json,jsx,md,ts,tsx,vue}\" ",
    "lint:fix": "eslint ./ --ext .astro,.cjs,.js,.jsx,.mjs,.ts,.tsx,.vue --quiet --fix --ignore-path ./.gitignore --cache --cache-strategy content --cache-location auxiliary/eslint-cache",
    "lint": "pnpm run lint:format && pnpm run lint:fix",
    "test:vitest-coverage": "vitest run --coverage",
    "test:vitest-coverage-viewer": "start test/vitest/coverage/index.html",
    "test:cypress-open": "cypress open",
    "test:cypress-run-component": "cypress run --component",
    "test:cypress-run-e2e": "cypress run --e2e",
    "test": "pnpm run test:vitest-coverage && pnpm run test:cypress-run-component",
    "build:vite": "vue-tsc --noEmit --project ./tsconfig.build.json && vite build --config vite.config.ts",
    "build": "pnpm run clean:build && pnpm run build:vite",
    "prepack": "pnpm run build",
    "npm:registry-login": "pnpm adduser --scope @demessifier",
    "npm:publish-dry": "pnpm publish --dry-run --access public",
    "npm:publish-public": "pnpm publish --access public",
    "semantic-release": "semantic-release",
    "checklist:without-install": "pnpm run generate && pnpm run lint && pnpm run test && pnpm run build && pnpm run pnpm:publish-dry",
    "checklist": "pnpm run packages:install && pnpm run packages:audit && pnpm run checklist:without-install",
    "checklist:clean": "pnpm run clean && pnpm run checklist"
  },
  "type": "module",
  "files": [
    "build"
  ],
  "types": "./build/src/index.d.ts",
  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "default": "./build/index.js"
    },
    "./component/*": {
      "types": "./build/src/component/*.d.ts",
      "import": "./build/src/component/*.js",
      "require": "./build/src/component/*.cjs",
      "default": "./build/src/component/*.js"
    },
    "./provider/*": {
      "types": "./build/src/provider/*.d.ts",
      "import": "./build/src/provider/*.js",
      "require": "./build/src/provider/*.cjs",
      "default": "./build/src/provider/*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Demessifier/demessifier-gui.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "release": {
    "branches": [
      {
        "name": "4-versioning",
        "channel": "latest"
      },
      {
        "name": "+([0-9])?(.{+([0-9]),x}).x"
      },
      {
        "name": "main",
        "channel": "latest"
      }
    ],
    "tagFormat": "releases/${version}"
  },
  "keywords": [
    "demessifier",
    "gui",
    "vue"
  ],
  "author": "McLayn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Demessifier/demessifier-gui/issues"
  },
  "homepage": "https://github.com/Demessifier/demessifier-gui#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "cypress": "^13.4.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^23.0.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "semantic-release": "^22.0.7",
    "ts-node": "^10.9.1",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vitest-matchmedia-mock": "^1.0.2",
    "vue-tsc": "^1.8.22"
  }
}
